<!DOCTYPE html>
%html
  %head
    %title HAI Office
    = javascript_include_tag :defaults
    = render 'shared/stylesheets'
    = csrf_meta_tag

  %body
    Office/std.html.haml<br/>
    #container
      = render 'shared/header'
      #flash
        - flash.each do |key, value|
          %div{ :class => "flash #{key}" }
            = value

      #column_left
        = yield :column_left

      #content
        = yield

      = render 'shared/footer'
      = debug(params) if Rails.env.development?

    = yield :javascript

%table.table.table-striped
  %thead
    %tr
      %th Name
      %th Gender
      %th Event
      %th Added

  %tbody
    - waitlisted.each do |p|
      %tr
        %td= link_to_show([:office, p], p.full_name)
        %td= p.gender
        %td= p.event_name
        %td= time_ago_in_words(p.created_at) + " ago"

#completed
  %table.table
    %thead
      %tr
        %th.name Name
        %th= render 'header_th', :label => 'Not Completed', :value => 'nc'
        %th= render 'header_th', :label => 'Completed', :value => 'completed'
        %th Toggle
    %tbody
      - registrations.by_first_name.each do |r|
        %tr
          %td.name= r.full_name
          = completed_registration_tds(r)
          %td= completed_toggle(r)
.wat-cf

.block
  = block_navigation do |nav|
    - nav.item "Edit #{event.display_name}", edit_office_event_path(event), :active => true

  .content
    = controls do |c|
      - add_button_to(c, :delete, event)

    %h2 Editing #{event.display_name}

    .inner
      = render 'form'

  = block_footer(event_breadcrumbs)
